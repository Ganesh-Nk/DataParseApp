@using DataParseApp.Models
@model List<XmlDataItem>

<div class="container mt-4">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Value</th>
                <th>Description</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Value</td>
                    <td>@item.Description</td>
                </tr>
            }
        </tbody>
    </table>
</div>
